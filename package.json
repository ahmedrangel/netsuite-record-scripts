{
  "name": "netsuite-record-scripts",
  "title": "NetSuite Record Scripts",
  "description": "Instantly view all scripts deployed on a NetSuite record with just one click!",
  "private": true,
  "version": "0.2.3",
  "type": "module",
  "scripts": {
    "dev": "wxt --config .config/wxt.ts",
    "dev:firefox": "wxt -b firefox",
    "build": "wxt build",
    "build:firefox": "wxt build -b firefox",
    "zip": "wxt zip",
    "zip:firefox": "wxt zip -b firefox",
    "compile": "vue-tsc --noEmit",
    "postinstall": "wxt prepare",
    "lint": "eslint . --config .config/eslint.mjs"
  },
  "dependencies": {
    "@headlessui/vue": "^1.7.23",
    "@iconify/vue": "^5.0.0",
    "@tailwindcss/vite": "^4.1.13",
    "cheerio": "^1.1.2",
    "tailwindcss": "^4.1.13",
    "vue": "^3.5.22"
  },
  "devDependencies": {
    "@eslint/compat": "^2.0.0",
    "@stylistic/eslint-plugin": "^5.6.1",
    "@types/chrome": "^0.1.32",
    "@typescript-eslint/eslint-plugin": "^8.50.0",
    "@vue/eslint-config-typescript": "^14.6.0",
    "@wxt-dev/auto-icons": "^1.1.0",
    "@wxt-dev/module-vue": "^1.0.3",
    "changelogen": "^0.6.2",
    "eslint": "^9.39.2",
    "eslint-import-resolver-typescript": "^4.4.4",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-vue": "^10.6.2",
    "iconify-browser-cache": "^0.4.0",
    "typescript": "5.9.3",
    "ufo": "^1.6.1",
    "vue-tsc": "^3.1.8",
    "wxt": "^0.20.13"
  },
  "packageManager": "pnpm@10.26.1"
}
